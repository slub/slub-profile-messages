{
	"name": "slub/slub-profile-messages",
	"type": "typo3-cms-extension",
	"description": "SLUB profile service messages extension for TYPO3",
	"authors": [
		{
			"name": "Andreas Pfeiffer",
			"role": "Developer"
		}
	],
	"version": "0.1.0",
	"license": "GPL-2.0-or-later",
	"require": {
		"typo3/cms-core": "^11"
	},
	"replace": {
		"typo3-ter/slub-profile-messages": "self.version"
	},
	"autoload": {
		"psr-4": {
			"Slub\\SlubProfileMessages\\": "Classes/"
		}
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "slub_profile_messages"
		}
	}
}
